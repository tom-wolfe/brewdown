{
  "name": "brewdown",
  "version": "1.3.0",
  "description": "A flavour of markdown and stylesheet specifically for D&amp;D 5th edition homebrew, implemented as a plugin for markdown-it.",
  "main": "./dist/js/brewdown.js",
  "scripts": {
    "build": "npm run clean && npm run lint && node ./build/index.js",
    "clean": "rimraf ./dist",
    "lint": "standard --env jasmine --fix --verbose | snazzy",
    "test": "node ./spec/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trwolfe13/brewdown.git"
  },
  "keywords": [
    "homebrew",
    "markdown",
    "D&D",
    "dungeons-and-dragons",
    "rpg",
    "markdown-it",
    "markdown-it-plugin"
  ],
  "author": "Tom Wolfe <trwolfe13@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trwolfe13/brewdown/issues"
  },
  "homepage": "https://github.com/trwolfe13/brewdown#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "fs-extra": "^4.0.0",
    "jasmine": "^2.7.0",
    "klaw": "^2.0.0",
    "node-sass": "^4.5.3",
    "rimraf": "^2.6.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "webpack": "^3.4.1"
  },
  "dependencies": {
    "markdown-it": "^8.3.1",
    "markdown-it-container": "^2.0.0"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "expect"
    ]
  }
}
